<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">



    <Button
        android:id="@+id/two"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:layout_centerHorizontal="true"
        android:textSize="60dp"
        android:text=" " />

    <Button
        android:id="@+id/one"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="5dp"
        android:layout_toLeftOf="@id/two"
        android:layout_alignTop="@id/two"
        android:textSize="60dp"
        android:text=" " />

    <Button
        android:id="@+id/three"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@id/two"
        android:layout_alignTop="@id/two"
        android:textSize="60dp"
        android:text=" " />

    <Button
        android:id="@+id/five"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_below="@id/two"
        android:layout_marginLeft="5dp"
        android:layout_centerHorizontal="true"
        android:textSize="60dp"
        android:text=" " />

    <Button
        android:id="@+id/four"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="5dp"
        android:layout_toLeftOf="@id/five"
        android:layout_alignTop="@id/five"
        android:textSize="60dp"
        android:text=" " />


    <Button
        android:id="@+id/six"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@id/five"
        android:layout_alignTop="@id/five"
        android:textSize="60dp"
        android:text=" " />

    <Button
        android:id="@+id/eight"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_below="@id/five"
        android:layout_marginLeft="5dp"
        android:layout_centerHorizontal="true"
        android:textSize="60dp"
        android:text=" " />

    <Button
        android:id="@+id/seven"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="5dp"
        android:layout_toLeftOf="@id/eight"
        android:layout_alignTop="@id/eight"
        android:textSize="60dp"
        android:text=" " />

    <Button
        android:id="@+id/nine"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@id/eight"
        android:layout_alignTop="@id/eight"
        android:textSize="60dp"
        android:text=" " />

    <TextView
        android:id="@+id/dialog"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/eight"
        android:layout_marginTop="20dp"
        android:gravity="center_horizontal"
        android:textSize="20sp"
        android:text=" " />

</RelativeLayout>
